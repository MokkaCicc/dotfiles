{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#ed4e4f",
          "template": "\uf0e7 <#d6e0d9>in</> ",
          "properties": {
            "root_icon": "root"
          }
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#c386f1",
          "template": "{{ if .SSHSession }}\uF817 {{ .UserName }}@{{ .HostName}} {{ end }}"
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#87deec",
          "template": "{{ .Path }} ",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": "/",
            "style": "agnoster_full"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#ef5aac",
          "template": "{{ if ne .RepoName .Env.USERNAME }}<#d6e0d9>on</> {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} {{ end }}",
          "properties": {
            "fetch_status": true
          }
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#98c379",
          "template": "<#d6e0d9>via</> üêç {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "context",
            "fetch_version": true,
            "fetch_virtual_env": true
          }
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "#ec4e4e",
          "template": "<#d6e0d9>via</> ü¶Ä {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "context",
            "fetch_version": true
          }
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "#f7df1c",
          "template": "<#d6e0d9>via</> \ue781 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "properties": {
            "fetch_version": true
          }
        },
        {
          "type": "dotnet",
          "style": "plain",
          "foreground": "#512bd4",
          "template": "<#ffffff>via</> \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#4de680",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ec4d4b{{ end }}"
          ],
          "template": "\u279c ",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    }
  ]
}
